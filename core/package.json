{
  "name": "@morio/core",
  "version": "0.1.0",
  "description": "Morio Core",
  "private": true,
  "scripts": {
    "build": "node build.mjs",
    "clean": "rimraf dist",
    "dev": "nodemon src/index.mjs",
    "lint": "eslint --fix 'src/**' 'config/**' 'shared/**'",
    "prettier": "npx prettier --ignore-unknown --write .",
    "test": "npx mocha tests/index.mjs"
  },
  "dependencies": {
    "dockerode": "^4.0.0",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "pino": "^8.16.2"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "esbuild": "^0.19.9",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.2.3"
  },
  "imports": {
    "#config": "./config/index.mjs",
    "#config/clients/linux": "./config/clients/linux.mjs",
    "#config/services/*": "./config/services/*.mjs",
    "#controllers/*": "./src/controllers/*.mjs",
    "#lib/services/*": "./src/lib/services/*.mjs",
    "#lib/*": "./src/lib/*.mjs",
    "#routes/*": "./src/routes/*.mjs",
    "#shared/*": "./shared/*.mjs"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/certeu/morio.git"
  },
  "author": "Joost De Cock <joost@joost.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/certeu/morio/issues"
  },
  "homepage": "https://github.com/certeu/morio#readme"
}
